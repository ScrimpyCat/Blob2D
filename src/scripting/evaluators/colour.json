            "examples": ["(lighten (253 212 73) 5) ;(1.041765 0.872941 0.300588)"],
            "return": { "type": "list" },
            "args": [[{ "type": "list", "name": "colour", "value": [{ "type": "number", "name": "red" }, { "type": "number", "name": "green" }, { "type": "number", "name": "blue" }, { "type": "number", "name": "alpha", "optional": true }] }, { "type": "number", "name": "adjustment"}]]
        },
