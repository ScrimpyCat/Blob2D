#Include CommonC
commonc_header = -I./deps/Common/CommonC

#Build CommonC
build deps/Common/CommonC/build/Allocator.o: cc deps/Common/CommonC/CommonC/Allocator.c
    headers = $commonc_header
build deps/Common/CommonC/build/CFAllocator.o: cc deps/Common/CommonC/CommonC/CFAllocator.c
    headers = $commonc_header
